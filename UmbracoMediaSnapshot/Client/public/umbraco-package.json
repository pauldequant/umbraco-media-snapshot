{
  "id": "UmbracoMediaSnapshot",
  "name": "Umbraco Media Snapshot",
  "version": "2.0.9",
  "allowTelemetry": true,
  "extensions": [
    {
      "name": "Umbraco Media Snapshot Bundle",
      "alias": "UmbracoMediaSnapshot.PropertyEditorUi",
      "type": "propertyEditorUi",
      "js": "/App_Plugins/UmbracoMediaSnapshot/umbraco-media-snapshot.js",
      "elementName": "snapshot-viewer",
      "meta": {
        "label": "File Snapshot History",
        "icon": "icon-history",
        "group": "Media Snapshot",
        "propertyEditorSchemaAlias": "UmbracoMediaSnapshot.PropertyEditorUi"
      }
    },
    {
      "name": "Snapshot Storage Dashboard",
      "alias": "UmbracoMediaSnapshot.Dashboard",
      "type": "dashboard",
      "js": "/App_Plugins/UmbracoMediaSnapshot/umbraco-media-snapshot2.js",
      "elementName": "snapshot-dashboard",
      "weight": 10,
      "meta": {
        "label": "Snapshot Storage",
        "pathname": "snapshot-storage"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionAlias",
          "match": "Umb.Section.Media"
        }
      ]
    }
  ]
}